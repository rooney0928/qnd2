<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- AuthView -->
    <declare-styleable name="authView">
        <attr name="mLogo" format="reference" />
        <attr name="mName" format="string" />
        <attr name="mStatus" format="enum">
            <enum name="ing" value="2" />
            <enum name="yes" value="1" />
            <enum name="no" value="0" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CPLAuthView">
        <attr name="cpl_checked">
            <enum name="yes" value="1"></enum>
            <enum name="no" value="0"></enum>
        </attr>
        <attr name="cpl_name" format="string" />
        <attr name="cpl_logo" format="reference" />

    </declare-styleable>
</resources>